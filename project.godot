;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Reference",
"class": "BlockData",
"language": "GDScript",
"path": "res://map/BlockData.gd"
} ]
_global_script_class_icons={
"BlockData": ""
}

[application]

config/name="Dungeon"
run/main_scene="res://Main.tscn"
